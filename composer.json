{
    "name": "petkopara/multi-search-bundle",
    "description": "Symfony bundle for Multi Criteria Search for doctrine entities using Form or Service.",
    "keywords": ["multi-search","criteria", "query builder", "symfony", "doctrine"],
    "type": "symfony-bundle",
    "homepage": "https://github.com/petkopara/PetkoparaMultiSearchBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Petko Petkov",
            "email": "petkopara@gmail.com"
        }
    ],
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "doctrine/annotations": "^1.13.2|^2.0",
        "doctrine/common": "^3.2.1",
        "doctrine/orm": "^2.5|^3.0",
        "symfony/form": "^5.4|^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/cache": "^6.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Petkopara\\MultiSearchBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        }
    }
}
